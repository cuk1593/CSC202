    private void cbAccountActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cbAccountActionPerformed
        int selectedIndex = cbAccount.getSelectedIndex();
        if (selectedIndex == 1)
        {
            txtBalance.setText(checkingBalString);
        }
        else if (selectedIndex == 2)
        {
            txtBalance.setText(savingBalString);
        }
        else 
            txtBalance.setText(" ");
    }//GEN-LAST:event_cbAccountActionPerformed

    private void txtDepositActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_txtDepositActionPerformed
        // TODO add your handling code here:
    }//GEN-LAST:event_txtDepositActionPerformed

    private void btnDepositActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnDepositActionPerformed
    	String depositAmtString1 = txtDeposit.getText();
    	int selectedIndex = cbAccount.getSelectedIndex();
        if (selectedIndex == 1)
        {
            txtBalance.setText(checkingBalString);
        }
        else if (selectedIndex == 2)
        {
        	depositAmt = Double.parseDouble(depositAmtString);
        	checkingBal += depositAmt1;
        	checkingBalString = String.valueOf(checkingBal);
        	txtBalance.setText(checkingBalString);
        }
        else 
        {
        	depositAmt = Double.parseDouble(depositAmtString);
        	savingBal += depositAmt2;
        	savingBalString = String.valueOf(savingBal);
        	txtBalance.setText(savingBalString);
        }
    	
    }//GEN-LAST:event_btnDepositActionPerformed

